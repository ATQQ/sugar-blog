{
  "name": "vitepress-plugin-rss",
  "version": "0.3.4",
  "description": "vitepress plugin, generate feed.rss",
  "author": "sugar",
  "license": "MIT",
  "homepage": "https://github.com/ATQQ/sugar-blog/tree/master/packages/vitepress-plugin-rss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATQQ/sugar-blog.git"
  },
  "bugs": {
    "url": "https://github.com/ATQQ/sugar-blog/issues"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "rss",
    "feed"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx tsup src/index.ts --dts --watch --format esm,cjs --external vitepress,sharp",
    "build": "npx tsup src/index.ts --dts --format esm,cjs --external vitepress,sharp --silent"
  },
  "peerDependencies": {
    "vitepress": "^1 || ^2"
  },
  "dependencies": {
    "@sugarat/theme-shared": "workspace:*",
    "feed": "^5.1.0",
    "markdown-it-container": "^4.0.0"
  },
  "devDependencies": {
    "sharp": "^0.34.5",
    "vite": "^5",
    "vitepress": "2.0.0-alpha.15"
  }
}
